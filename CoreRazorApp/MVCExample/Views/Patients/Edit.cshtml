@model MVCExample.Models.Patient<form action="Edit" method="post">    <div>        @Html.HiddenFor(e => e.Id)        <label>@Html.DisplayNameFor(e => e.Name)</label>        @Html.TextBoxFor(e => e.Name)    </div>    <div>        <label>@Html.DisplayNameFor(e => e.Allergies)</label>        <p>@Html.TextBoxFor(e => e.Allergies)</p>    </div>    <div><input type="submit" class="btn btn-primary" /></div></form>