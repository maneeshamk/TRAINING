@model IEnumerable<MVCExample.Models.Patient><table class="table table-striped">    <tr>        <th>ID</th>        <th>Name</th>        <th>Allergies</th>    </tr>    @foreach (var item in Model)    {        <tr>            <td>@item.Id</td>            <td>@item.Name</td>            <td>@item.Allergies</td>            <td><a href="../Patients/Delete?id=@item.Id">Delete</a></td>            <td><a href="../Patients/Edit?id=@item.Id">Edit</a></td>        </tr>    }</table>